
<!-- Dynamic content here -->

    <!-- Intro Header -->
    <header class="intro">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <h1 id="city-header" class="brand-heading">${city}</h1>
						<p class="intro-text">Temperature:
                            <br>${cond.temperature} °C</p>
						<p class="intro-text">Wind:
                            <br>${cond.windSpeed} km/h</p>
						<p class="intro-text">Real temperature:
                            <br>${cond.windChill} °C</p>
							
							
                        <a href="#about" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <!-- About Section -->
    
    
    <section id="about" class="container content-section text-center">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <h2>Acording to weather conditions:</h2>
                
                <c:choose>
    
    			<c:when test="${cond.type eq 'warm'}">
                
				<p>
				
					<!-- WARM -->
					<table>
					<tr>
						<td><img class="icon icons8-Sun" src="data:image/png;base64," width="48" height="48"></td>
						<td>It is WARM outside better take this:</td>
					</tr>
					<tr>
						<td><td>
						<td>T-shirt</td>
						<td><img class="icon icons8-T-Shirt" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Shades</td>
						<td><img class="icon icons8-Sun-Glasses" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
					 <tr>
						<td><td>
						<td>Plain pair of trousers</td>
						<td><img class="icon icons8-Trousers" src="data:image/png;base64," width="50" height="50"></td>
					 </tr>
					</table>
					
					
					
					
				</p>
				
				</c:when>
				<c:when test="${cond.type eq 'freezing'}">
                
				<p>
					<!--Freezing <table> -->
					<table>
					<tr>
						<td><img class="icon icons8-Icy" src="data:image/png;base64," width="48" height="48"></td>
						<td>It is FREEZING outside better take this:</td>
					 </tr>
					<tr>
						<td><td>
						<td>Warm hat</td>
						<td><img class="icon icons8-Beanie" src="data:image/png;base64," width="48" height="48"></td>
					<tr>
						<td><td>
						<td>Warm coat</td>
						<td><img class="icon icons8-Coat" src="data:image/png;base64," width="48" height="48">></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Warm boots</td>
						<td><img class="icon icons8-Winter-Boots" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>A scarf</td>
						<td><img class="icon icons8-Scarf" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Pair of gloves</td>
						<td><img class="icon icons8-Mittens" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					</table>
						
				</p>
				
				</c:when>
				
				<c:when test="${cond.type eq 'hot'}">
                
				<p>
				
					<table>
					<tr>
						<td><img class="icon icons8-Palm-Tree" src="data:image/png;base64," width="48" height="48"></td>
						<td>It is HOT outside better take this:</td>
					 </tr>
					<tr>
						<td><td>
						<td>T-shirt</td>
						<td><img class="icon icons8-T-Shirt" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Shorts</td>
						<td><img class="icon icons8-Shorts" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Shades</td>
						<td><img class="icon icons8-Sun-Glasses" src="data:image/png;base64," width="48" height="48"></td>
						</tr>
					 <tr>
						<td><td>
						<td>Sunscreen</td>
						<td><img class="icon icons8-Cream-Tube" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					</table>					
				</p>
				
				</c:when>
				
				<c:when test="${cond.type eq 'moderate'}">
                
				<p>
				
					<!-- Moderate -->
					<table>
					<tr>
						<td><img class="icon icons8-Temperature" src="data:image/png;base64," width="48" height="48"></td>
						<td>Temperature is MODERATE better take this:</td>
					 </tr>
					<tr>
						<td><td>
						<td>Shirt</td>
						<td><img class="icon icons8-Shirt" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Sneakers</td>
						<td><img class="icon icons8-Trainers" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Plain pair of trousers</td>
						<td><img class="icon icons8-Trousers" src="data:image/png;base64," width="50" height="50"></td>
					 </tr>
					</table>	
				</p>
				
				</c:when>
				
				
				<c:when test="${cond.type eq 'cold'}">
                
				<p>
				
					<table>
					<tr>
						<td><img class="icon icons8-Winter" src="data:image/png;base64," width="48" height="48"></td>
						<td>It is cold outside better take this:</td>
					 </tr>
					<tr>
						<td><td>
						<td>Warm coat</td>
						<td><img class="icon icons8-Coat" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Warm boots</td>
						<td><img class="icon icons8-Winter-Boots" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>A scarf</td>
						<td><img class="icon icons8-Scarf" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					 <tr>
						<td><td>
						<td>Pair of gloves</td>
						<td><img class="icon icons8-Mittens" src="data:image/png;base64," width="48" height="48"></td>
					 </tr>
					</table>	
				</p>
				
				</c:when>
				
				</c:choose>
				
				
				<p><a href="#contact" class="btn btn-circle page-scroll">
                            <i class="fa fa-angle-double-down animated"></i>
                        </a>
				</p>		
            </div>
        </div>
		
    </section>
    <!-- Dynamic content here -->